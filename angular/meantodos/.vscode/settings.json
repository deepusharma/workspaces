// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/*.js": {"when": "$(basename).ts"},
        "**/*.map": {"when": "$(basename).map"},
        "**/node_modules/": true,        
        "**/client/node_modules/": true,        
        "**/client/typings/": true,        
    }
}